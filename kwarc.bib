@MISC{Baumgartner:fdpll00,
  author = {Peter Baumgartner},
  title = {{FDPLL -- A First-Order Davis-Putnam-Logeman-Loveland Procedure}},
  note = {Submitted},
  abstract = {The paper describes a lifted version FDPLL of the well-known Davis-Putnam-Logeman-Loveland
	(DPLL) procedure. While DPLL is based on a splitting rule for case
	analysis wrt.\ {\em ground\/} and complementary literals, FDPLL uses
	a lifted splitting rule, i.e.\ the case analysis is made wrt.\ non-ground
	and complementary literals now. \par The motivation for this lifting
	is the same as in all successful proof procedures for first-order
	logic used today: to gain efficiency by representing infinitely many
	inferences on the ground level in a compact way by means of computation
	with most general unifiers, and, to enable much more powerful redundancy
	tests as is possible when working on the ground level only. \par
	The main difficulty was to define a reading of the literals used
	for splitting that enables a sound, proof confluent and strongly
	complete calculus. It leads to a new technique to represent first-order
	interpretations. Based on it, the calculus and a respective proof
	procedure are developed. Special instances of FDPLL are propositional
	DPLL (for propositional clause logic) and unit-resulting resolution
	(for Horn clause logic). \par As the main result, strong completeness
	of the calculus and correctness of the proof procedure is proven.
	The method is prototypically implemented and was tested against the
	TPTP problem library.},
  url = {http://www.uni-koblenz.de/~peter/FDPLL/FDPLLshort.ps}
}

@INPROCEEDINGS{BauFurNie:ht96,
  author = {Peter Baumgartner and Ulrich Furbach and Ilkka Niemel{\"a}},
  title = {{Hyper Tableaux}},
  booktitle = {Proc. JELIA 96},
  year = {1996},
  number = {1126},
  series = lnai,
  organization = {European Workshop on Logic in AI},
  publisher = {Springer},
  abstract = { This paper introduces a variant of clausal normal form tableaux that
	we call ``hyper tableaux''. Hyper tableaux keep many desirable features
	of analytic tableaux while taking advantage of the central idea from
	(positive) hyper resolution, namely to resolve away all negative
	literals of a clause in a single inference step. Another feature
	of the proposed calculus is the extensive use of universally quantified
	variables. This enables new efficient forward-chaining proof procedures
	for full first order theories as variants of tableaux calculi.},
  url = {http://www.uni-koblenz.de/~peter/Publications/tableaux-jelia-llncs.ps.gz}
}

