@README{--README--,
  x = { This is the shared BibTeX database of the KWARC group.  Use      },
  x = { project-kwarc@jacobs-university.de for any questions or comments },
  x = { about this file.                                                 }
}
@README{--EDITING--,
  x = { This file does not have any particular order; just keep in mind  },
  x = { that if A crossrefs B, A must come before B.                     },
  x = {                                                                  },
  x = { Do not use any graphical frontend for editing, but use a text    },
  x = { editor, as the latter makes sure that changes are easy to spot   },
  x = { when using Subversion's diff.                                    },
  x = {                                                                  },
  x = { Do not touch anything that you don't understand.                 },
  x = {                                                                  },
  x = { It is easiest to create new entries by copying and modifying     },
  x = { existing ones.                                                   },
}
@README{--ONLINE-PUBLICATIONS--,
  x = { For online publications, it is strongly recommended to use       },
  x = { BibLaTeX's @online entries; see                                  },
  x = { http://trac.kwarc.info/KWARC/wiki/BibLaTeX                      },
  x = {                                                                  },
  x = { Most online publications come in multiple variants for           },
  x = { compatibility with legacy publication workflows (such as LNCS).  },
  x = { The naming scheme is:                                            },
  x = {                                                                  },
  x = { * @online{entry:on} -- BibLaTeX `online' entry                   },
  x = { * @webpage{entry:webpage} -- alpha[h]url, an older solution for  },
  x = {   online citations                                               },
  x = { * @misc{entry:web} -- plain BibTeX entry                         },
  x = { * @misc{entry:base} -- crossref'd by the others above, contains  },
  x = {   common fields, not suitable for citation                       },
}
@README{--PUBLICATION-LIST-GENERATION--,
  x = { The publication lists on the KWARC homepage are generated from   },
