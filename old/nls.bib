@article{Eijck:adlfa99,
     title = {Axiomatising Dynamic Logics for Anaphora},
    author = {Jan van Eijck},
     pages = {103--126},
    volume = {1},
    number = {1},
      year = {1999},
   journal = {Journal of Language and Computation},
       URL = {http://www.dcs.kcl.ac.uk/journals/jlac/V11/index.html},
  abstract = {\noindent A task put on the agenda of philosophical logic by the
  dynamic turn in natural language analysis and in logic, the account
  for the dynamics of anaphoric context shifts in reasoning, is taken
  up along new lines. A standard way to treat reasoning with dynamic
  anaphora logics is by means of translation to first order predicate
  logic. This paper gives direct sequent axiomatisations of some well
  known dynamic anaphora logics.  The calculi are shown to be sound
  and complete.  The calculi throw new light on the relation between
  various formats for representing anaphoric links by means of sharing
  of dynamically bound variables.  The sequent approach to the
  dynamics of anaphoric context takes the notion of anaphoric context
  change from premiss to conclusion as starting point of the analysis,
  and does not rely on an implicit translation to first order
  predicate logic.}
}

