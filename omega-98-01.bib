@InProceedings{MelisMeierCL2000,
  author =  {E. Melis and A. Meier},
  editor =  {J. Loyd and V. Dahl and U. Furbach and M. Kerber and K. Lau and C.
Palamidessi and
                  L.M. Pereira and Y. Sagivand P. Stuckey},
  title =  {Proof Planning with Multiple Strategies},
  booktitle =  {First International Conference on Computational Logic},
  series =  {Lecture Notes on Artificial Intelligence},
  volume =  {1861},
  publisher =  {Springer-Verlag},
  year =  2000,
  url =   {http://www.ags.uni-sb.de/\textasciitilde
melis/Pub/MelisMeierCL2000.ps.gz},
  keywords =  {OMEGA,SFB,ARTICLE,Proof Planning},
  pages =  {644-659},
  abstract =  {Humans have different problem solving strategies at their
disposal and they can flexibly
                  employ several strategies when solving a complex problem,
whereas previous theorem proving
                  and planning systems typically employ a single strategy or a
hard coded combination of a few
                  strategies. We introduce multi-strategy proof planning that
allows for combining a number of
                  strategies and for {\em switching}\/ flexibly between
strategies in a proof planning
                  process. Thereby proof planning becomes more robust since it
does not necessarily fail if
                  one problem solving mechanism fails. Rather it can {\em
reason}\/ about preference of
                  strategies and about failures. Moreover, our strategies
provide a means for structuring the
                  vast amount of knowledge such that the planner can cope with
the otherwise overwhelming
                  knowledge in mathematics.}
}

@InProceedings{MelisPolletAIPSws00,
  author =  {E. Melis and M. Pollet},
  title =  {Domain Knowledge for Search Heuristics in Proof Planning},
  booktitle =  {AIPS 2000 workshop: Analyzing and Exploiting Domain Knowledge},

  pages =  {12-15},
  abstract =  { Proof planning faces potentially infinitely branching search
spaces. Therefore, several
                  techniques for restricting the search have been developed or
adopted, among them the
                  evaluation of control rules. Similar to other planning
domains, the control rules express
                  (possibly domain-dependent) mathematical control knowledge.
However, typically the control
                  rules' metapredicates are more complicated than those in
other planning domains and inspect
                  situations, entities, and objects that are very different
from other domains. The discovery
                  of the mathematical control knowledge is a research problem
in itself. In this paper we
                  present two different kinds of control knowledge useful for
proof planning: one that refers
                  to {\em typical examples} and one that triggers {\em proofs
by contradiction}.},
  keywords =  {OMEGA,SFB,ARTICLE,PROOF PLANNING},
  year =  2000,
}

@InProceedings{MelisZimmerMueller00,
  author =  {E. Melis and J. Zimmer and T. M{\"u}ller},
  title =  {Integrating Constraint Solving into Proof Planning},
  booktitle =  {Frontiers of Combining Systems, FroCos-2000},
  year =  2000,
  editor =  {Ringeissen},
  series =  {Lecture Notes on Artificial Intelligence},
  publisher =  {Springer},
  abstract =  {In automated theorem proving and proof planning mathematical
objects with theory-specific
                  properties have to be constructed. More often than not, mere
unification offers little
                  support for this task. There, the integration of constraint
solvers can sometimes help
                  solving this problem. As we shall show, certain conditions
have to be met in order to
                  integrate the constraint solver's efficient activities in a
way that is correct and
                  sufficient for proof planning. Off-the-shelf constraint
solvers do not satisfy those
                  conditions in the first place. Therefore, we developed
extensions of the constraint solving
                  technology that are necessary for its use in proof planning.
We present the extensions and
                  their implementation in the constraint solver CoSie }
}

@InProceedings{MelisZimmerMuellerECAI,
  author =  {E. Melis and J. Zimmer and T. M{\"u}ller},
  title =  {Extensions of Constraint Solving for Proof Planning},
  booktitle =  {European Conference on Artificial Intelligence},
  year =  2000,
  keywords =  {OMEGA,SFB,ARTICLE,CONSTRAINT SOLVING,PROOF PLANNING},
  url =   {http://www.ags.uni-sb.de/\textasciitilde
melis/Pub/MelisZimmerMuellerECAI.ps.gz},
  editor =  {W. Horn},
  pages =  {229-233},
  abstract =  {The integration of constraint solvers into proof planning has
pushed the problem solving
                  horizon. Proof planning benefits from the general
functionalities of a constraint solver
                  such as consistency check, constraint inference, as well as
the search for
                  instantiations. However, off-the-shelf constraint solvers
need to be extended in order to be
                  be integrated appropriately: In particular, for correctness,
the context of constraints and
                  Eigenvariable-conditions have to be taken into account.
Moreover, symbolic and numeric
                  constraint inference are combined. This paper discusses the
extensions to constraint solving
                  for proof planning, namely the combination of symbolic and
numeric inference, first-class
                  constraints, and context trees. We also describe the
implementation of these extensions in
                  the constraint solver \mbox{\cosie}.}
}

