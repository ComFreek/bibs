@InProceedings{Fiedler:dastco00,
  author =  {Armin Fiedler},
  title =  {Determining and Structuring the Content of Presentations of Proofs
in Natural Language},
  booktitle =  {\PROC of 17th International Conference on Automated
Deduction---Workshop W4: Automated
                  Deduction in Education},
  editor =  {Erica Melis and Dana Scott},
  address =  {Pittsburgh, PA, USA},
  pages =  {44-51},
  year =  2000,
  keywords =  {!OMEGA!,SFB,ARTICLE},
  url =   {http://www.ags.uni-sb.de/\textasciitilde
omega/pub/postscript/Fiedler-CADE00-WS4.ps.gz},
  abstract =  {One of the real-world applications of deduction systems is in
mathematics education. For the
                  successful use of mathematics education systems, the
interface to the user plays an
                  important role. In this paper, we first shall present some of
the requirements such an
                  interface should meet. Most prominent are multi-modal
communication using natural language
                  generation, interactivity, and adaptation with respect to
explicitness, abstraction and
                  presentation strategy. Then, we shall suggest an appropriate
architecture focusing on the
                  dialog planner as the crucial component. We shall discuss
speech acts as the primitive
                  actions planned by the dialog planner and show how plan
operators are envoked for the
                  presentation of proofs.}
}

